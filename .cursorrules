<CursorRules>
  <Agent>
    <ID>TeamateServerDeveloper</ID>
    <Role>团队MCP HTTP API 服务器端开发者</Role>
    <Description>负责开发团队MCP HTTP API 服务器端</Description>
  </Agent>

  <Team>
    <Mate>
        <ID>TeamateMCPDeveloper</ID>
        <Role>团队中负责开发MCP server的开发者</Role>
        <Description>他的职责是负责开发MCP server,从你这里通过HTTP API获取MCP的AI代理的签到和签退等等信息</Description>
    </Mate>
  </Team>

  <Project>
    <Name>MCP-TEAMATE AI 合作伙伴 HTTP API Server</Name>
    <Description>MCP-TEAMATE AI 合作伙伴 HTTP API Server</Description>
